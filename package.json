    "babel-plugin-transform-decorators": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "chai": "3.5.0",
    "chai-jquery": "2.0.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "2.1.3",
    "css-loader": "0.23.1",
    "eslint": "2.8.0",
    "eslint-plugin-react": "5.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
